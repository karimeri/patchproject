<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd" label="RMA Return View Image" design_abstraction="custom">
    <body>
        <referenceContainer name="content">
            <block class="Magento\Framework\View\Element\Template" name="magento_rma_item_form_template">
                <block class="Magento\Rma\Block\Form\Renderer\Image" name="magento_rma_item_form_template_image" as="image" template="Magento_Rma::form/view/image.phtml"/>
            </block>
        </referenceContainer>
        <referenceContainer name="after.body.start">
            <block class="Magento\Framework\View\Element\Js\Components" name="head.components" as="components" template="Magento_Rma::js/components.phtml"/>
        </referenceContainer>
    </body>
</page>
